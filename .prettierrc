{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "components/(.*)$",
    "layout/(.*)$",
    "ui/(.*)$",
    "providers/(.*)$",
    "shared/(.*)$",
    "assets/(.*)$",
    "config/(.*)$",
    "store/(.*)$",
    "hooks/(.*)$",
    "utils/(.*)$",
    "types/(.*)$",
    "services/(.*)$",
    "api/(.*)$",
    "^../(.*)",
    "^./(.*)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}